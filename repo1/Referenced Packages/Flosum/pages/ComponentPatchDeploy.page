               		startDeployment();
               	}
			}
            else
            {
            	if(result != undefined && result.id != undefined)
               	{
               		if(result.done == 'true')
               		{
						// Errors?
						if(result.status == 'Succeeded')
						{
							if(result.zipFile != undefined )
