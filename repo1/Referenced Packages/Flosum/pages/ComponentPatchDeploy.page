				{
						tempWriter.add(file.name, new zip.BlobReader(file.data), function() {
						addIndex++;
						if(addIndex < files.length)
							nextFile();
						else
						{
							tempWriter.close(function(base64Data) {
			    					base64Data = base64Data.slice(base64Data.indexOf(';base64,') + 8);
			    					var li = [];
			    					li.push(base64Data);
			    					patchList['Patch'] = li;
