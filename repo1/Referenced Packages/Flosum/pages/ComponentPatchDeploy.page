		function startDeployment()
		{
			if(patchList['Patch'] != undefined && targetSession != '' && targetReferesh != '' && targetInstance != '')
			{
				dploymentZiplist = patchList['Patch'];
				if(dploymentZiplist.length > 0)
				{
